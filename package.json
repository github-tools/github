    "gulp-uglify": "^1.5.1",
    "jsdoc": "^3.4.0",
    "minami": "^1.1.1",
    "mocha": "^2.3.4",
    "must": "^0.13.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
