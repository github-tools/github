  ],
  "readmeFilename": "README.md",
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp build",
    "test": "mocha --opts ./mocha.opts test/*.spec.js",
    "test-verbose": "DEBUG=github* npm test",
    "lint": "gulp lint",
    "make-docs": "node_modules/.bin/jsdoc -c .jsdoc.json --verbose",
    "release": "./release.sh"
  },
  "babel": {
    "presets": [
      "es2015"
